{
  "name": "fidelpe-extractx",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:stable": "next dev",
    "build": "next build --turbopack",
    "build:stable": "next build",
    "clean": "rimraf .next",
    "start": "next start",
    "lint": "eslint .",
